<div id="savedArticles" class="panel panel-defaul">
	{{#each articles}}
		
			<div class="panel panel-primary">
				<div class="panel-heading">
					<h2 class="panel-title">{{this.title}}</h2>
				</div>
				<div class="panel-body">
					<p class='articleHead'> {{this.head}} </p>
					{{!-- <a class='articleLink' href="{{this.link}}"> {{this.link}} </a> --}}
					<button class='btn btn-default' align='right' id='notesButton' type='submit' data-id={{this._id}} >Add Notes</button>
					<button class='btn btn-default' align='right' id='deleteButton' type='submit' data-id={{this._id}} >Delete Article</button>				
				</div>		
			</div>

	{{/each}}	
</div><!-- endtop articles panel  -->

