<!-- Display all Articles -->
<div id="allArticles" class="panel panel-defaul">
	{{#each articles}}
		<div class="panel panel-primary">
			<div class="panel-heading">
				<h2 class="panel-title">{{this.title}}</h2>
			</div>
			<div class="panel-body">
				<p class='articleHead'> {{this.head}} </p>
				<a class='articleLink' href="{{this.link}}"> {{this.link}} </a>
				<button class='btn btn-warning' align='right' id='saveButton' type='submit' data-id={{this._id}} >Save Article</button>
			</div>		
		</div>
	{{/each}}

</div><!-- endtop articles panel  -->
